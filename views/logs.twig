<!doctype html>
<html lang="en">
  <head>
    <title>VR-Photosense logging</title>
  </head>
  <body>
    <div class="row d-flex justify-content-center mt-100 mb-100">

      <div class="col-lg-6">
        <div class="card">
          <div class="card-body text-center">
            <h4 class="card-title m-b-0">Logs guardados</h4>
           </div>
          <ul class="list-style-none">
            {% for file in files %}
              <li class="d-flex no-block card-body border-bottom">
                <div>
                  <a href="/{{ file }}">{{ file }}</a>
                </div>
              
              </li>
            {% endfor %}
          </ul>     
        </div>
      </div>
  </div>
  </body>
</html>